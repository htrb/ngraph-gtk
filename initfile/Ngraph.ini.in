[Ngraph]
terminal=xterm -e nterminal
alloc_console=0
history_size=1000

[x11menu]
script_console=0
addin_console=1
framex=0
framey=0
minus_hyphen=1
status_bar=1
show_tip=1
backing_store=1
browser=firefox DOCDIRDEF/html_ja/ngraph.htm
mouse_click=300
color_depth=4
viewer_dpi=70
viewer_auto_redraw=1
viewer_load_file_on_redraw=1
viewer_show_ruler=1
editor=gedit
script=LEGEND: automatic legend generator,legend.nsc,
script=FIT-RESULT: fitting results ---> legend-text,fitrslt.nsc,
script=APPEND: append NGP file,append.nsc,
script=MATH: calculator,math.nsc,
script=TEXT-IN: insert legend-text,text-in.nsc,
script=CALC: making a data file,calc.nsc,
script=FFT: Fast Fourier Transform,fft.nsc,
ext_driver=PostScript (EPSF),gra2ps,.eps,-e -c
ext_driver=PostScript (A4),gra2ps,.ps,-c -p a4
ext_driver=PostScript (A4 rotate),gra2ps,.ps,-c -p a4 -r
ext_driver=Windows Metafile,gra2wmf,.wmf,
prn_driver=PostScript (A4),gra2ps, | lpr,-c -p a4
prn_driver=PostScript (A4 rotate),gra2ps, | lpr,-c -p a4 -r
prn_driver=GhostScript,gra2ps, | gs -,-c
