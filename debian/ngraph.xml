<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
	<mime-type type="application/x-ngraph">
		<sub-class-of type="text/plain"/>
		<_comment>Ngaph</_comment>
		<glob pattern="*.ngp"/>
	</mime-type>
	<mime-type type="application/x-ngraph-graphic">
		<sub-class-of type="text/plain"/>
		<_comment>Ngaph graphic file</_comment>
		<glob pattern="*.gra"/>
	</mime-type>
	<mime-type type="application/x-ngraph-script">
		<sub-class-of type="text/plain"/>
		<_comment>Ngaph script</_comment>
		<glob pattern="*.nsc"/>
	</mime-type>
</mime-info>
