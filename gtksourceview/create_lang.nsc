#! /usr/bin/ngraph

cat $1

new data
echo '    <context id="builtins" style-ref="builtin">'
get data -field functions | sed -re 's!(.+)!      <keywords>\1</keywords>!'
echo '    </context>'
echo

echo '    <context id="constants" style-ref="constant">'
get data -field constants | sed -re 's!(.+)!      <keywords>\1</keywords>!'
echo '    </context>'
echo

cat $2
